{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowSavedImage": false,
  "ImageSettings": {
    "Contrast": 50,
    "Sharpness": 100,
    "Binaryzation": 0.65,
    "GaussianBlur": 1.5
  },
  "EnabledRecognized": [
    1, // Original
    2, // Contrast
    3, // Sharpness
    4, // Binaryzation
    5  // GaussianBlur
  ],
  "Patterns": [
    "IMEI\\d*(\\d{15})", // IMEI000000000000000
    "IME\\|\\d*(\\d{15})", // IME|000000000000000
    "IME1\\d*(\\d{15})", // IME1000000000000000
    "ME1\\d*(\\d{15})", // ME1000000000000000
    "IMEl1\\d*(\\d{15})", // IMEl000000000000000
    "MEl1\\d*(\\d{15})", // MEl000000000000000
    "\\|ME\\|\\d*(\\d{15})", // |ME|000000000000000
    "ME\\|\\d*(\\d{15})", // ME|000000000000000
    "1ME1\\d*(\\d{15})", // 1ME1000000000000000
    "lMEl\\d*(\\d{15})", // lMEl000000000000000
    "IMEINO\\d*(\\d{15})", // IMEINO000000000000000
    "MEINO\\d*(\\d{15})", // MEINO000000000000000
    "IMEI\\d*:(\\d{15})", // IMEI1:000000000000000
    "IME\\|\\d*:(\\d{15})", // IME|1:000000000000000
    "ME\\|\\d*:(\\d{15})", // ME|1:000000000000000
    "IME1\\d*:(\\d{15})", // IME|1:000000000000000
    "IMEl\\d*:(\\d{15})", // IME|l:000000000000000
    "MEl\\d*:(\\d{15})", // ME|l:000000000000000
    "\\|ME\\|\\d*:(\\d{15})", // |ME|1:000000000000000
    "ME\\|\\d*:(\\d{15})", // ME|1:000000000000000
    "lMEl\\d*:(\\d{15})", // lMEl1:000000000000000
    "1ME1\\d*:(\\d{15})", // 1ME|1:000000000000000
    "lMEl\\d*:(\\d{15})", // lME|1:000000000000000
    "MEI\\d*:(\\d{15})", // MEI1:000000000000000
    "MEl\\d*:(\\d{15})", // MEl1:000000000000000
    "ME1\\d*:(\\d{15})", // ME|1:000000000000000
    "MEl\\d*:(\\d{15})" // ME|1:000000000000000
  ]
}
