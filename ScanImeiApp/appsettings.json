{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Patterns": [
    "IMEI\\d*\\s(\\d{15})", // IMEI 000000000000000
    "IMEI\\d*:\\s(\\d{15})", // IMEI1: 000000000000000, IMEI2: 000000000000000 и т.д.
    "IMEI\\d*:(\\d{15})" // IMEI1:000000000000000, IMEI2:000000000000000 и т.д.
  ],
  "AllowedHosts": "*"
}
